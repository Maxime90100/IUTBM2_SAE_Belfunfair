\r\n    return{\r\n      btnLogin:false,\r\n      btnSignup:false\r\n    }\r\n  },\r\n  methods:{\r\n    changeBtnLogin(){\r\n      if(this.btnLogin) this.btnLogin = false\r\n      else{\r\n        this.btnLogin = true\r\n        this.btnSignup = false\r\n      }\r\n    },\r\n    changeBtnSignup(){\r\n      if(this.btnSignup) this.btnSignup = false\r\n      else{\r\n        this.btnSignup = true\r\n        this.btnLogin = false\r\n      }\r\n    },\r\n    goTo(path){\r\n      this.$router.replace(path)\r\n    }\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}