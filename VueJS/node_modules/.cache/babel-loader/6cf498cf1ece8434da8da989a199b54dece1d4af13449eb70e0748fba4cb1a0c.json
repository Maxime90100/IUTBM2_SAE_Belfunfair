{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"organisateurPrestataires\",\n  data() {\n    return {\n      prestataires: [],\n      artistes: [],\n      maneges: [],\n      stands: [],\n      presta: null\n    };\n  },\n  methods: {\n    showId(id) {\n      axios({\n        method: 'get',\n        url: 'http://localhost:3000/users/attractions/' + id\n      }).then(res => {\n        let data = res.data.data;\n        this.maneges = data.maneges;\n        this.stands = data.stands;\n        this.artistes = data.artistes;\n        this.presta = this.prestataires.filter(p => p.id === id)[0];\n      });\n    }\n  },\n  mounted() {\n    axios({\n      method: 'get',\n      url: 'http://localhost:3000/organisateurs/prestataires'\n    }).then(res => {\n      this.prestataires = res.data.data.prestataires;\n    });\n  }\n};","map":{"version":3,"mappings":"AA+GA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACAC;QACAC;QACAC;MACA;QACA;QACA;QACA;QACA;QACA;MACA;IACA;EACA;EACAC;IACAH;MACAC;MACAC;IACA;MACA;IACA;EACA;AACA","names":["name","data","prestataires","artistes","maneges","stands","presta","methods","showId","axios","method","url","mounted"],"sourceRoot":"src/components/organisateurs","sources":["organisateurPrestataires.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <v-simple-table dark style=\"width: 80%; margin-left: 10%\">\r\n      <thead>\r\n        <tr>\r\n          <th>Id</th>\r\n          <th>{{ $t('attribute.firstname') }}</th>\r\n          <th>{{ $t('attribute.surname') }}</th>\r\n          <th>Email</th>\r\n          <th>Attractions</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"p in prestataires\" :key=\"'prestataire-'+p.id\">\r\n          <td>{{p.id}}</td>\r\n          <td>{{p.firstname}}</td>\r\n          <td>{{p.surname}}</td>\r\n          <td>\r\n            <a :href=\"'mailto:'+p.email+'?subject=[SERVICE] Belfunfair&body=[info] Ce mail s\\'adresse à '+p.firstname+' '+p.surname+', en qualité de prestataire de l\\'évènement Belfunfair :\\n'\">\r\n              {{p.email}}\r\n            </a>\r\n          </td>\r\n          <td>\r\n            <v-btn v-on:click=\"showId(p.id)\" small>{{$t('attribute.show')}}</v-btn>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </v-simple-table>\r\n\r\n    <div style=\"width: 80%; margin-left: 10%; margin-bottom: 10px\">\r\n      <div v-if=\"presta\" style=\"margin: 10px 0; text-align: center\">\r\n        <h1>{{presta.firstname}} {{presta.surname}}</h1>\r\n      </div>\r\n      <div v-if=\"maneges.length > 0\">\r\n        <h4>Maneges</h4>\r\n        <v-simple-table>\r\n          <thead>\r\n          <tr>\r\n            <th>Id</th>\r\n            <th>name</th>\r\n            <th>description</th>\r\n            <th>type</th>\r\n            <th>taille_min</th>\r\n            <th>status</th>\r\n          </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"m in maneges\" :key=\"'maneges-'+m.id\">\r\n              <td>{{m.id}}</td>\r\n              <td>{{m.name}}</td>\r\n              <td>{{m.description}}</td>\r\n              <td>{{m.type}}</td>\r\n              <td>{{m.taille_min}}</td>\r\n              <td :class=\"m.status\">{{m.status}}</td>\r\n            </tr>\r\n          </tbody>\r\n        </v-simple-table>\r\n      </div>\r\n      <div v-if=\"stands.length > 0\">\r\n        <h4>Stands</h4>\r\n        <v-simple-table>\r\n          <thead>\r\n          <tr>\r\n            <th>Id</th>\r\n            <th>name</th>\r\n            <th>description</th>\r\n            <th>type</th>\r\n            <th>status</th>\r\n          </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"s in stands\" :key=\"'stands-'+s.id\">\r\n              <td>{{s.id}}</td>\r\n              <td>{{s.name}}</td>\r\n              <td>{{s.description}}</td>\r\n              <td>{{s.type}}</td>\r\n              <td>{{s.status}}</td>\r\n            </tr>\r\n          </tbody>\r\n        </v-simple-table>\r\n      </div>\r\n      <div v-if=\"artistes.length > 0\">\r\n        <h4>Artistes</h4>\r\n        <v-simple-table>\r\n          <thead>\r\n          <tr>\r\n            <th>Id</th>\r\n            <th>name</th>\r\n            <th>groupe</th>\r\n            <th>description</th>\r\n            <th>type</th>\r\n            <th>status</th>\r\n          </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"a in artistes\" :key=\"'artistes-'+a.id\">\r\n              <td>{{a.id}}</td>\r\n              <td>{{a.groupe}}</td>\r\n              <td>{{a.name}}</td>\r\n              <td>{{a.description}}</td>\r\n              <td>{{a.type}}</td>\r\n              <td>{{a.status}}</td>\r\n            </tr>\r\n          </tbody>\r\n        </v-simple-table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"organisateurPrestataires\",\r\n  data(){\r\n    return{\r\n      prestataires:[],\r\n      artistes:[],\r\n      maneges:[],\r\n      stands:[],\r\n      presta:null\r\n    }\r\n  },\r\n  methods:{\r\n    showId(id){\r\n      axios({\r\n        method: 'get',\r\n        url: 'http://localhost:3000/users/attractions/'+id,\r\n      }).then(res=>{\r\n        let data = res.data.data\r\n        this.maneges = data.maneges\r\n        this.stands = data.stands\r\n        this.artistes = data.artistes\r\n        this.presta = this.prestataires.filter(p=>p.id===id)[0]\r\n      })\r\n    }\r\n  },\r\n  mounted() {\r\n    axios({\r\n      method: 'get',\r\n      url: 'http://localhost:3000/organisateurs/prestataires',\r\n    }).then(res=>{\r\n      this.prestataires = res.data.data.prestataires\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.attributed{\r\n  background-color: green;\r\n  color: white;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}