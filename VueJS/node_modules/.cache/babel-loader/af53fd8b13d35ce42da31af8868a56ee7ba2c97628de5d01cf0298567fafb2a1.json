{"ast":null,"code":"import i18n from \"@/i18n\";\nexport default {\n  name: 'prestataireToolbar',\n  methods: {\n    goTo(path) {\n      this.$router.replace(path);\n    },\n    changeLanguage(lang) {\n      return i18n.locale = lang;\n    },\n    logout() {\n      this.$store.state.user = null;\n      this.goTo('/login');\n    }\n  }\n};","map":{"version":3,"mappings":"AAuDA;AAEA;EACAA;EACAC;IACAC;MAAA;IAAA;IACAC;MAAA;IAAA;IACAC;MACA;MACA;IACA;EACA;AACA","names":["name","methods","goTo","changeLanguage","logout"],"sourceRoot":"src/components/prestataires","sources":["prestataireToolbar.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <v-app-bar\r\n        dark\r\n        collapse-on-scroll\r\n        fixed\r\n    >\r\n      <!-- BURGER MENU -->\r\n      <v-menu left bottom>\r\n        <template v-slot:activator=\"{ on }\">\r\n          <v-btn icon v-on=\"on\">\r\n            <v-app-bar-nav-icon> </v-app-bar-nav-icon>\r\n          </v-btn>\r\n        </template>\r\n\r\n        <v-list>\r\n          <v-list-item @click=\"goTo('/prestataire')\">{{$t('prestataireToolbar.profil')}}</v-list-item>\r\n          <v-list-item @click=\"goTo('/prestataire/maneges')\">{{$t('prestataireToolbar.manege')}}</v-list-item>\r\n          <v-list-item @click=\"goTo('/prestataire/stands')\">{{$t('prestataireToolbar.stand')}}</v-list-item>\r\n          <v-list-item @click=\"logout\">{{$t('button.logout')}}</v-list-item>\r\n          <v-list-item @click=\"goTo('/')\">{{$t('prestataireToolbar.home')}}</v-list-item>\r\n         </v-list>\r\n      </v-menu>\r\n\r\n      <v-toolbar-title>{{this.$store.state.user.firstname}} {{this.$store.state.user.surname}}</v-toolbar-title>\r\n      <v-spacer></v-spacer>\r\n\r\n      <v-menu>\r\n        <template v-slot:activator=\"{ on }\">\r\n          <v-btn @click=\"logout\">{{$t('button.logout')}}</v-btn>\r\n          <v-btn @click=\"goTo('/')\">{{$t('prestataireToolbar.home')}}</v-btn>\r\n          <v-btn icon v-on=\"on\">\r\n            <v-icon>mdi-flag</v-icon>\r\n          </v-btn>\r\n        </template>\r\n        <v-list v-for=\"(lang,index) in $t('lang')\" :key=\"index\">\r\n          <v-list-item @click=\"changeLanguage(lang.value)\">\r\n            <img :src=\"lang.image\" width=\"25px\" height=\"20px\">\r\n          </v-list-item>\r\n        </v-list>\r\n      </v-menu>\r\n\r\n      <!-- EXTEND MENU -->\r\n      <template v-slot:extension>\r\n        <v-tabs align-with-title>\r\n          <v-tab @click=\"goTo('/prestataire/maneges')\">{{$t('prestataireToolbar.manege')}}</v-tab>\r\n          <v-tab @click=\"goTo('/prestataire/stands')\">{{$t('prestataireToolbar.stand')}}</v-tab>\r\n        </v-tabs>\r\n      </template>\r\n    </v-app-bar>\r\n    <br><br><br><br><br>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport i18n from \"@/i18n\";\r\n\r\nexport default {\r\n  name: 'prestataireToolbar',\r\n  methods: {\r\n    goTo (path) {this.$router.replace(path)},\r\n    changeLanguage(lang){return i18n.locale = lang},\r\n    logout(){\r\n      this.$store.state.user = null\r\n      this.goTo('/login')\r\n    }\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}